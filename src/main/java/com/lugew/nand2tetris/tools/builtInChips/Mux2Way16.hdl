/**
 * 2-way 16-bit multiplexor.  
 * out = a if sel == 0
 *       b if sel == 1
 */


CHIP Mux2Way16 {
     
    IN a[16], b[16], sel;
    OUT out[16];

    BUILTIN Mux2Way16;
}
